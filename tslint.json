{
  "extends": ["tslint:latest", "tslint-plugin-prettier", "tslint-config-prettier"],
  "rules": {
    "no-implicit-dependencies": [true, ["@"]],
    "no-submodule-imports": [false],
    "no-console": [false],
    "no-namespace": [false],
    "object-literal-sort-keys": [false],
    "prettier": [true, "./.prettierrc.js"]
  }
}